<ul id='sortable_list'>
  <li class='clearfix' id="<%= dom_id(list) -%>">
    <div class='clearfix'>
      <span class='actions'>
        <%= link_to refinery_icon_tag('application_edit.png'), edit_admin_catalog_type_entry_attribute_type_entry_attribute_type_value_path(@catalog_type, @entry_attribute_type, list),
                   :title => t('.edit_this_entry') %>
        <%= link_to refinery_icon_tag('delete.png'), admin_catalog_type_entry_attribute_type_entry_attribute_type_value_path(@catalog_type, @entry_attribute_type, list),
                   :class => "cancel confirm-delete",
                   :title => t('.confirm_delete_entry_title'),
                   :confirm => t('shared.admin.delete.message', :title => list.value),
                   :method => :delete %>
      </span>
      <%=h list.value %>
    </div>
  </li>
</ul>

